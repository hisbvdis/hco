{
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "name": "HCO",
  "colors": {
    "activityBarBadge.background": "#007acc",
    "editor.background": "#fafafa",
    "editor.foreground": "#444444",
    "editor.inactiveSelectionBackground": "#e5ebf1",
    "editor.rangeHighlightBackground": "#00000000",
    "editor.selectionHighlightBackground": "#add6ff80",
    "editorGroupHeader.tabsBackground": "#d4d4d4",
    "editorSuggestWidget.background": "#f3f3f3",
    "input.placeholderForeground": "#767676",
    "list.focusHighlightForeground": "#9dddff",
    "list.hoverBackground": "#e8e8e8",
    "notebook.cellBorderColor": "#e8e8e8",
    "notebook.selectedCellBackground": "#c8ddf150",
    "ports.iconRunningProcessForeground": "#369432",
    "searchEditor.textInputBorder": "#cecece",
    "settings.numberInputBorder": "#cecece",
    "settings.textInputBorder": "#cecece",
    "sideBar.border": "#dbdbdc",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.border": "#61616130",
    "sideBarTitle.foreground": "#6f6f6f",
    "statusBarItem.errorBackground": "#c72e0f",
    "statusBarItem.remoteBackground": "#16825d",
    "statusBarItem.remoteForeground": "#fafafa",
    "tab.activeBackground": "#fafafa",
    "tab.border": "#cccccc",
    "tab.inactiveBackground": "#e3e3e3",
    "tab.lastPinnedBorder": "#61616130",
    "list.activeSelectionIconForeground": "#fafafa",
    "terminal.ansiGreen": "#009900",
    "terminal.ansiYellow": "#cc9900",
    "editor.lineHighlightBorder": "#00000000",
    "editor.lineHighlightBackground": "#00000000",
    "editorIndentGuide.background1": "#dedede",
    "editorIndentGuide.activeBackground1": "#b3b3b3",
    "editorBracketPairGuide.activeBackground1": "#8f8f8f",
    "editorBracketPairGuide.activeBackground2": "#8f8f8f",
    "editorBracketPairGuide.activeBackground3": "#8f8f8f",
    "editorBracketPairGuide.activeBackground4": "#8f8f8f",
    "editorBracketPairGuide.activeBackground5": "#8f8f8f",
    "editorBracketPairGuide.activeBackground6": "#8f8f8f"
  },
  "tokenColors": [
    {
      "scope": [
        "support.type.property-name.json",
        "meta.embedded",
        "meta.block",
        "meta.definition.property",

        "string.regexp keyword.other",
        "constant.other.character-class.regexp",
        "string.regexp punctuation.definition.string.begin",
        "string.regexp punctuation.definition.string.end",
      ],
      "settings": {
        "foreground": "#444444"
      }
    },
    {
      "scope": [
        "storage",
        "keyword.operator.new",

        "markup.bold",
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "keyword",

        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
      ],
      "settings": {
        "foreground": "#1200db"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
      ],
      "settings": {
        "foreground": "#00b0e6"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "constant.character",
        "support.class.component",

        "punctuation.definition.character-class.regexp",
        "punctuation.definition.group.regexp",
      ],
      "settings": {
        "foreground": "#d60024"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
      ],
      "settings": {
        "foreground": "#ff8e80"
      }
    },
    {
      "scope": [
        "string",

        "markup.inline.raw",
      ],
      "settings": {
        "foreground": "#009900"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "keyword.other.unit",
      ],
      "settings": {
        "foreground": "#CC9900"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading markup.inline.raw",
      ],
      "settings": {
        "foreground": "#a21319"
      }
    },
    {
      "scope": [
        "comment",
      ],
      "settings": {
        "foreground": "#969896"
      }
    },
    {
      "scope": [
        // "constant.language",
        "keyword.control",
      ],
      "settings": {
        "foreground": "#AF00DB"
      }
    },
    {
      "scope": [
        "support.type",
        "support.class",
        "meta.type.annotation entity.name.type",
      ],
      "settings": {
        "foreground": "#78909C"
      }
    },
    {
      "scope": [
        "storage",
        "keyword.operator.new",

        "entity.name.tag",

        "markup.bold",
        "markup.heading",
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control",
        "markup.italic",
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
    {
      "scope": [
        "storage.type.function.arrow",
      ],
      "settings": {
        "fontStyle": ""
      }
    }
  ],
  "semanticHighlighting": false,
  "semanticTokenColors": {},
}
