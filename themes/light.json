{
	"$schema": "vscode://schemas/color-theme",
	"type": "light",
	"name": "HCO-light-saturated",
	"colors": {
		"activityBarBadge.background": "#007acc",
		"editor.background": "#ededed",
		"editor.foreground": "#000000",
		"editor.inactiveSelectionBackground": "#e5ebf180",
		"editor.rangeHighlightBackground": "#00000000",
		"editor.selectionHighlightBackground": "#add6ff80",
		"editorGroupHeader.tabsBackground": "#cfcfcf",
		"editorSuggestWidget.background": "#f3f3f3",
		"input.placeholderForeground": "#767676",
		"list.focusHighlightForeground": "#9dddff",
		"list.hoverBackground": "#e8e8e8",
		"notebook.cellBorderColor": "#e8e8e8",
		"notebook.selectedCellBackground": "#c8ddf150",
		"ports.iconRunningProcessForeground": "#369432",
		"searchEditor.textInputBorder": "#cecece",
		"settings.numberInputBorder": "#cecece",
		"settings.textInputBorder": "#cecece",
		"sideBar.background": "#ededed",
		"sideBar.border": "#c6c9c9",
		"sideBar.foreground": "#000000",
		"editorHoverWidget.background": "#ededed",
		"sideBarSectionHeader.background": "#00000000",
		"sideBarSectionHeader.border": "#61616130",
		"statusBarItem.errorBackground": "#c72e0f",
		"statusBarItem.remoteBackground": "#16825d",
		"statusBarItem.remoteForeground": "#ededed",
		"tab.activeBackground": "#ededed",
		"tab.border": "#cccccc",
		"tab.inactiveBackground": "#d9d9d9",
		"tab.lastPinnedBorder": "#61616130",
		"list.activeSelectionIconForeground": "#ededed",
		"terminal.ansiGreen": "#009900",
		"terminal.ansiYellow": "#cca300",
		"editor.lineHighlightBorder": "#00000000",
		"editor.lineHighlightBackground": "#00000000",
		"editorIndentGuide.background1": "#dedede",
		"editorIndentGuide.activeBackground1": "#b3b3b3",

		"editorBracketHighlight.foreground1": "#0086e6",
		"editorBracketPairGuide.activeBackground1": "#333333",
		"editorBracketHighlight.foreground2": "#9d2ed1",
		"editorBracketPairGuide.activeBackground2": "#333333",
		"editorBracketHighlight.foreground3": "#0086e6",
		"editorBracketPairGuide.activeBackground3": "#333333",
		"editorBracketHighlight.foreground4": "#9d2ed1",
		"editorBracketPairGuide.activeBackground4": "#333333",
	},
	"tokenColors": [
		{
			"scope": [
				"support.type.property-name.json",
				"meta.property-name support.type",
				"meta.embedded",
				"meta.interpolation",
				"meta.block",
				"meta.definition.property",
				"string.regexp keyword.other",
				"constant.other.character-class.regexp",
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"scope": [
				"storage",
				"keyword",
				"constant.language",
				"keyword.operator",
				"variable.language",
				"string punctuation.definition.template-expression",
				"string punctuation.definition.interpolation"
			],
			"settings": {
				"foreground": "#1300e6"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function",
			],
			"settings": {
				"foreground": "#009CCC"
			}
		},
		{
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#9d2ed1"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"constant.character",
				"support.class.component"
			],
			"settings": {
				"foreground": "#c20020"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ffa3b3"
			}
		},
		{
			"scope": [
				"string",
				"markup.inline.raw"
			],
			"settings": {
				"foreground": "#50A14F"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#0451a5"
			}
		},
		{
			"scope": [
				"markup.heading",
				"markup.heading markup.inline.raw"
			],
			"settings": {
				"foreground": "#a21319"
			}
		},
		{
			"scope": [
				"comment",
				"storage.type.class.jsdoc",
			],
			"settings": {
				"foreground": "#9e9e9e"
			}
		},
		{
			"scope": [
				"meta.parameters variable.parameter"
			],
			"settings": {
				"foreground": "#bd5e00"
			}
		},
		{
			"scope": [
				"support.type",
			],
			"settings": {
				"foreground": "#ad9f00"
			}
		},
		{
			"scope": [
				"storage",
				"entity.name.tag",
				"markup.bold",
				"markup.heading"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"keyword.control",
				"markup.italic",
				"comment",
				"storage.type.class.jsdoc",
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [],
			"settings": {
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": [
				"keyword.operator",
				"keyword.other.unit",
				"storage.type.function.arrow",
				"storage.type.sql",
				"storage.modifier.sql",
			],
			"settings": {
				"fontStyle": ""
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		// "class": "#000000"
		"interface": "#ad9f00",
    "type": "#ad9f00",
    "typeParameter": "#ad9f00",
    "variable": "#000000",
	}
}