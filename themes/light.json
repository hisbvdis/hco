{
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "name": "HCO-light-saturated",
  "colors": {
    "activityBarBadge.background": "#007acc",
    "editor.background": "#fafafa",
    "editor.foreground": "#242424",
    "editor.inactiveSelectionBackground": "#e5ebf180",
    "editor.rangeHighlightBackground": "#00000000",
    "editor.selectionHighlightBackground": "#add6ff80",
    "editorGroupHeader.tabsBackground": "#d4d4d4",
    "editorSuggestWidget.background": "#f3f3f3",
    "input.placeholderForeground": "#767676",
    "list.focusHighlightForeground": "#9dddff",
    "list.hoverBackground": "#e8e8e8",
    "notebook.cellBorderColor": "#e8e8e8",
    "notebook.selectedCellBackground": "#c8ddf150",
    "ports.iconRunningProcessForeground": "#369432",
    "searchEditor.textInputBorder": "#cecece",
    "settings.numberInputBorder": "#cecece",
    "settings.textInputBorder": "#cecece",
    "sideBar.border": "#dbdbdc",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.border": "#61616130",
    "sideBarTitle.foreground": "#6f6f6f",
    "statusBarItem.errorBackground": "#c72e0f",
    "statusBarItem.remoteBackground": "#16825d",
    "statusBarItem.remoteForeground": "#fafafa",
    "tab.activeBackground": "#fafafa",
    "tab.border": "#cccccc",
    "tab.inactiveBackground": "#e3e3e3",
    "tab.lastPinnedBorder": "#61616130",
    "list.activeSelectionIconForeground": "#fafafa",
    "terminal.ansiGreen": "#009900",
    "terminal.ansiYellow": "#cca300",
    "editor.lineHighlightBorder": "#00000000",
    "editor.lineHighlightBackground": "#00000000",
    "editorIndentGuide.background1": "#dedede",
    "editorIndentGuide.activeBackground1": "#b3b3b3",

    // Default
    // "editorBracketHighlight.foreground1": "#0431fa",
		// "editorBracketHighlight.foreground2": "#319331",
		// "editorBracketHighlight.foreground3": "#844d15",

		"editorBracketHighlight.foreground1": "#cc8800",
		"editorBracketHighlight.foreground2": "#bb2abb",
    "editorBracketHighlight.foreground3": "#0431fa",

    // "editorBracketPairGuide.activeBackground1": "#333333",
    // "editorBracketPairGuide.activeBackground2": "#333333",
    // "editorBracketPairGuide.activeBackground3": "#333333",
    // "editorBracketPairGuide.activeBackground4": "#333333",
    // "editorBracketPairGuide.activeBackground5": "#333333",
    // "editorBracketPairGuide.activeBackground6": "#333333",
  },
  "tokenColors": [
    {
      "scope": [
        "support.type.property-name.json",
        "meta.property-name support.type",
        "meta.embedded",
        "meta.interpolation",
        "meta.block",
        "meta.definition.property",

        "keyword.control",

        "string.regexp keyword.other",
        "constant.other.character-class.regexp",
      ],
      "settings": {
        "foreground": "#242424"
      }
    },
    {
      "scope": [
        "storage",
        "keyword",
        "constant.language",

        "string punctuation.definition.template-expression",
        "string punctuation.definition.interpolation"
      ],
      "settings": {
        "foreground": "#1300e6"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
      ],
      "settings": {
        "foreground": "#009CCC"
      }
    },
    {
      "scope": [
        "keyword.control",
      ],
      "settings": {
        "foreground": "#9A2FD0"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "constant.character",
        "support.class.component",
      ],
      "settings": {
        "foreground": "#cc0022"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
      ],
      "settings": {
        "foreground": "#ffa3c2"
      }
    },
    {
      "scope": [
        "string",
        "markup.inline.raw",
      ],
      "settings": {
        "foreground": "#009900"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "keyword.other.unit",
      ],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading markup.inline.raw",
      ],
      "settings": {
        "foreground": "#a21319"
      }
    },
    {
      "scope": [
        "comment",
      ],
      "settings": {
        // "foreground": "#969896"
        "foreground": "#9E9E9E"
      }
    },
    {
      "scope": [
        "meta.parameters variable.parameter",
      ],
      "settings": {
        "foreground": "#cc6600"
      }
    },
    {
      "scope": [
        "support.type",
      ],
      "settings": {
        "foreground": "#b3a400"
      }
    },
    {
      "scope": [
        "storage",
        "entity.name.tag",

        "markup.bold",
        "markup.heading",
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control",
        "markup.italic",
        "comment",
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.other.unit",
        "storage.type.function.arrow",
        "storage.type.sql",
        "storage.modifier.sql",
      ],
      "settings": {
        "fontStyle": ""
      }
    },
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "interface": "#b3a400",
    "type": "#b3a400",
    "typeParameter": "#b3a400",
    "variable": "#242424",
  },
}