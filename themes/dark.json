{
  "$schema": "vscode://schemas/color-theme",
  "type": "hcDark",
  "name": "HCO-dark",
  "colors": {
    "editor.background": "#333333",
    "editor.foreground": "#d9d9d9",
    "editorCursor.foreground": "#FFFFFF",

    // "editor.selectionBackground": "#ffffff90",
    // "editor.wordHighlightBackground": "#ffffff50",
    // "editor.wordHighlightStrongBackground": "#ffffff50",
    // "editor.wordHighlightTextBackground": "#ffffff90",

    // "editor.findMatchBackground": "#ffffff90",
    // "editor.findMatchHighlightBackground": "#ffffff50",
  },
  "tokenColors": [
    {
      "scope": [
        "support.type.property-name.json",
      ],
      "settings": {
        "foreground": "#d9d9d9",
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
      ],
      "settings": {
        "foreground": "#ABB2BF",
      }
    },
    {
      "scope": [
        "comment",
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": [
        "storage",
        "constant.language",
        "keyword.operator",
      ],
      "settings": {
        "foreground": "#4896e5"
      }
    },
    {
      "scope": [
        "entity.name.function",
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": [
        "keyword.control",
      ],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": [
        "meta.parameters variable.parameter",
      ],
      "settings": {
        "foreground": "#ed975a"
      }
    },
    {
      "scope": [
        "support.type",
      ],
      "settings": {
        // "foreground": "#e4e495"
        "foreground": "#e0e085"
      }
    },
    {
      "scope": [
        "entity.name.tag",
      ],
      "settings": {
        "foreground": "#ff7088"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
      ],
      "settings": {
        "foreground": "#ffc4b8"
      }
    },
    {
      "scope": [
        "string",
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "keyword.other.unit",
      ],
      "settings": {
        "foreground": "#91AFD1"
      }
    },
    {
      "scope": [
        "storage",
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control",
        "comment",
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "interface": "#e1e198",
    "type": "#e1e198",
    "typeParameter": "#e1e198",
  },
}